{
  "name": "@thebbz/siwe-ethos-react",
  "version": "1.0.0",
  "type": "module",
  "description": "React components and hooks for Sign in with Ethos authentication",
  "license": "MIT",
  "author": "thebbz",
  "homepage": "https://github.com/thebbz/siwETHOS#readme",
  "bugs": {
    "url": "https://github.com/thebbz/siwETHOS/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thebbz/siwETHOS",
    "directory": "packages/react"
  },
  "keywords": [
    "ethos",
    "siwe",
    "sign-in-with-ethereum",
    "react",
    "authentication",
    "wallet",
    "web3",
    "modal"
  ],
  "sideEffects": [
    "*.css"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./modal": {
      "types": "./dist/modal.d.ts",
      "import": "./dist/modal.js",
      "require": "./dist/modal.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "import": "./dist/hooks.js",
      "require": "./dist/hooks.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src/",
    "test": "vitest run",
    "prepublishOnly": "pnpm run test && pnpm run build"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@bgd-labs/react-web3-icons": "^1.3.4"
  },
  "devDependencies": {
    "@thebbz/siwe-ethos": "^1.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "jsdom": "^24.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
