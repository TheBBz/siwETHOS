{
  "name": "@thebbz/siwe-ethos",
  "version": "1.2.0",
  "type": "module",
  "description": "JavaScript SDK for Sign in with Ethos authentication",
  "license": "MIT",
  "author": "thebbz",
  "homepage": "https://github.com/thebbz/siwETHOS#readme",
  "bugs": {
    "url": "https://github.com/thebbz/siwETHOS/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thebbz/siwETHOS",
    "directory": "packages/sdk"
  },
  "keywords": [
    "ethos",
    "siwe",
    "sign-in-with-ethereum",
    "oauth",
    "authentication",
    "identity",
    "reputation",
    "web3",
    "wallet"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src/",
    "test": "vitest run",
    "prepublishOnly": "pnpm run test && pnpm run build"
  },
  "dependencies": {
    "@thebbz/siwe-ethos-providers": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
